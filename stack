#include "monty.h"

/**
 * stack - Switches the format to a stack (LIFO)
 * @stack: Double pointer to the top of the stack
 * @line_number: Line number of the opcode in the file
 */
void stack(stack_t **stack, unsigned int line_number)
{
    (void)stack;
    (void)line_number;
    queue_mode = 0;
}

