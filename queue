#include "monty.h"
/**
 * queue - Switches the format to a queue (FIFO)
 * @stack: Double pointer to the top of the stack
 * @line_number: Line number of the opcode in the file
 */
void queue(stack_t **stack, unsigned int line_number)
{
    (void)stack;
    (void)line_number;
    queue_mode = 1;
}

